{
    "name": "xslainpwa/laravel-pwa",
    "description": "Laravel Progressive Web App",
    "keywords": [
        "laravel", "pwa", "progressive", "web", "app", 
        "multi-tenancy", "pwa", "laravel", "multitenancy", "multitenant", "progressive-web-app",
        "laravel-pwa", "pwa-laravel", "google-pwa", "ios-pwa", "firefox-pwa", "safari-pwa",
        "ios", "android", "chrome", "safari", "firefox" 
    ],
    "license": "MIT",
    "homepage": "https://github.com/Xslainpwa/laravel-pwa",
    "support": {
        "issues": "https://github.com/Xslainpwa/laravel-pwa/issues",
        "source": "https://github.com/Xslainpwa/laravel-pwa"
    },
    "authors": [
        {
            "name": "Ogounga Emmanuel",
            "email": "info@xslain.com"
        }
    ],
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "XslainPwa\\PWA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XslainPwa\\PWA\\": "src/",
            "XslainPwa\\PWA\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "XslainPwa\\PWA\\PwaServiceProvider"
            ],
            "aliases": {
                "PWA": "XslainPwa\\PWA\\Facades\\PWA"
            }
        }
    }
}
